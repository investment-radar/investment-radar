<h1>Hello Charts</h1>

<%
  quote = IEX::Resources::Quote.get('MSFT')
  max_price = quote.week_52_high.ceil
  min_price = quote.week_52_low.floor
%>

<%= line_chart weekly_charts_path(symbol: 'MSFT'), min: min_price, mix: max_price, points: false, height: "500px" %>

<%= line_chart daily_charts_path(symbol: 'MSFT'), min: min_price, mix: max_price, points: false, height: "500px" %>